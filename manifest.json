{
  "manifest_version": 2,
  "name": "Tab Archive",
  "version": "0.1",
  "offline_enabled": false,
  "default_locale": "en",
  "description": "An extension to close your tabs and upload what pages they are on to the cloud.",
  "permissions": [
    "identity",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "oauth2": {
    "client_id": "172305384164-btta42bf23h342eo59p5h9r3gdrhckcj.apps.googleusercontent.com",
    "scopes": [
      "openid"
    ]
  }
}
